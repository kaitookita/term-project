<h1>Course Management</h1>

<div class="media">
  <div class="media-body">
<table class="table table-hover">
  <thead>
    <tr>
      <th>Courses</th>
      <th>Coursename</th>
      
      <th>Lecturer</th>
      <th>Semester</th>
      <th>Year</th>
      <th colspan="3"></th>

    </tr>
  </thead>

  <tbody>
    <% @courses.each do |course| %>
      <tr>
        <td><%= course.course %></td>
        <td><%= course.coursename %></td>
       
        <td><%= course.lecturer %></td>
        <td><%= course.semester %></td>
        <td><%= course.year %></td>
       
    <% end %>
   
  </tbody>
</table>
  </div>
</div>
<br>

